{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "intech-fda-ADF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureDataLakeStoragelink1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStoragelink1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devduchen23.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devduchen23.dfs.core.windows.net/"
		},
		"AzureDataLakeStoragelink1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devduchen23.dfs.core.windows.net/"
		},
		"AzureDatabricksLinkToWorkspace_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0209-225934-smmn23dk"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://intech-keyvault-fda.vault.azure.net/"
		},
		"SqlServer4_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost\\SQLEXPRESS"
		},
		"SqlServer4_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "intech-sqllocal-db"
		},
		"SqlServer4_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "felipe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "intech-fda-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureDataLakeStoragelink1_accountKey": {
						"value": "[parameters('AzureDataLakeStoragelink1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureDataLakeStoragelink1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragelink1_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricksLinkToWorkspace_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksLinkToWorkspace_properties_typeProperties_existingClusterId')]"
					},
					"SqlServer4_properties_typeProperties_server": {
						"value": "[parameters('SqlServer4_properties_typeProperties_server')]"
					},
					"SqlServer4_properties_typeProperties_database": {
						"value": "[parameters('SqlServer4_properties_typeProperties_database')]"
					},
					"SqlServer4_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer4_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}